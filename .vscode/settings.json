{
  "explorer.autoRevealExclude": {
    "**/node_modules": false
  },
  "search.exclude": {
    ".git/": true,
    ".vscode/": true,
    "build/": true,
    "designer/": true,
    "docker/": true,
    "docs/": true,
    "migrations/": true,
    "/dist/": true,
    "/var/": true,
    "var/": true
  },
  "files.watcherExclude": {
    ".git/": true,
    ".vscode/": true,
    "build/": true,
    "designer/": true,
    "docker/": true,
    "docs/": true,
    "migrations/": true,
    "/dist/": true,
    "/var/": true,
    "var/": true
  },
  "debug.javascript.autoAttachFilter": "always" // https://code.visualstudio.com/docs/nodejs/nodejs-debugging#_auto-attach
}
